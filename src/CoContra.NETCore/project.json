{
	"version": "1.0.1",
	"title": "CoContra",
	"description": "Delegate-like classes for covariant and contravariant delegates and events",
	"authors": [ "Nick Strupat" ],
	"packOptions": {
		"tags": [ "covariance", "covariant", "contravariance", "contravariant", "delegate" ],
		"projectUrl": "https://github.com/NickStrupat/CoContra",
		"licenseUrl": "https://raw.githubusercontent.com/NickStrupat/CoContra/master/LICENSE"
	},

	"buildOptions": {
		"compile": [ "../CoContra.NET45/*.cs" ]
	},

	"dependencies": {
		"NETStandard.Library": "1.6.1",
		"System.Collections.Immutable": "1.3.0",
		"System.Reflection.TypeExtensions": "4.3.0"
	},

	"frameworks": {
		"netstandard1.0": {
			"buildOptions": { "outputName": "CoContra.netstandard1.0" },
			"imports": "dnxcore50"
		}
	}
}
